{
  "name": "honeyworks-info",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently --kill-others npm:dev:*",
    "dev:http-server": "http-server -p 35173 i18n-json",
    "dev:vite": "vite --host",
    "tsc": "vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite build && http-server-spa dist index.html 4173",
    "lint": "eslint src --ext .ts,.js,.vue",
    "lint:fix": "eslint src --ext .ts,.js,.vue --fix",
    "lint-staged": "lint-staged",
    "prepare": "git submodule init && git submodule update && husky install"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "@vueuse/core": "^9.6.0",
    "axios": "^1.2.0",
    "axios-cache-interceptor": "^0.10.7",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.26",
    "roboto-fontface": "*",
    "vue": "^3.2.45",
    "vue-i18n": "9",
    "vue-router": "^4.1.6",
    "vuetify": "3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@types/lodash": "^4.14.190",
    "@types/node": "^18.11.9",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^3.2.0",
    "@volar/vue-language-service": "^1.0.9",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.28.0",
    "http-server-spa": "^1.3.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "postcss": "^8.4.19",
    "sass": "^1.56.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-pwa": "^0.13.3",
    "vite-plugin-vuetify": "^1.0.0",
    "vue-tsc": "^1.0.9"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  }
}
