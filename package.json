{
  "name": "honeyworks-info",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@7.14.0",
  "scripts": {
    "dev": "vite --host",
    "tsc": "vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite build && http-server-spa dist index.html 4173",
    "test": "vitest",
    "lint": "eslint src --max-warnings=0 --ext .ts,.js,.vue",
    "lint:fix": "eslint src --max-warnings=0 --ext .ts,.js,.vue --fix",
    "lint-staged": "lint-staged",
    "prepare": "git submodule init && git submodule update && husky install"
  },
  "dependencies": {
    "@mdi/font": "7.1.96",
    "@vueuse/core": "^9.7.0",
    "axios": "^1.2.1",
    "axios-cache-interceptor": "^0.10.7",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.28",
    "roboto-fontface": "*",
    "vue": "^3.2.45",
    "vue-i18n": "9",
    "vue-router": "^4.1.6",
    "vuetify": "^3.1.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.17",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^4.0.0",
    "@volar/vue-language-service": "^1.0.14",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.30.0",
    "http-server-spa": "^1.3.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.31",
    "sass": "^1.57.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.5",
    "vite-plugin-md": "^0.22.5",
    "vite-plugin-pwa": "^0.16.5",
    "vite-plugin-vuetify": "^1.0.1",
    "vitest": "^0.26.2",
    "vue-tsc": "^1.0.14"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
