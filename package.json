{
  "name": "honeyworks-info",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently --kill-others npm:dev:*",
    "dev:http-server": "http-server -p 35173 i18n-json",
    "dev:vite": "vite --host",
    "tsc": "vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.js,.vue",
    "lint:fix": "eslint src --ext .ts,.js,.vue --fix",
    "lint-staged": "lint-staged",
    "prepare": "git submodule init && git submodule update && husky install"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "roboto-fontface": "*",
    "vue": "^3.2.37",
    "vue-i18n": "9",
    "vue-router": "^4.1.5",
    "vuetify": "^3.0.0-beta.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.8.2",
    "@types/webfontloader": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@volar/vue-language-service": "^0.38.9",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.5.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.2.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "sass": "^1.52.3",
    "tailwind-loves-sass": "^1.1.3",
    "tailwindcss": "^3.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.3",
    "vite": "^3.0.8",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vue-tsc": "^0.38.9"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
